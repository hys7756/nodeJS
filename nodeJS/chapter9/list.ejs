<!DOCTYPE html>
<html>
    <header>
        <title>Coffee List</title>
    </header>
    <body>
        <h1>리스트 페이지</h1>
        <a href="/insert">INSERT DATA</a>
        <hr/>
        <table>
            <tr>
                <th>DELETE</th>
                <th>EDIT</th>
                <th>ID</th>
                <th>Menu</th>
                <th>Price</th>
            </tr>
            <% data.forEach(function(item, index) { %>
            <tr>
                <td><a href="delete/<%= item.id%>">DELETE</a></td>
                <td><a href="edit/<%= item.id%>">EDIT</a></td>
                <td><%= item.id %></td>
                <td><%= item.name %></td>
                <td><%= item.price %></td>
            </tr>
            <% }); %>
        </table>
    </body>
</html>